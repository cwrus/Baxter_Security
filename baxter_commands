ROS_HOSTNAME= **PC NAME**
ROS_MASTER_URI=http://baxtette.local:11311

#enable:
rosrun baxter_tools enable_robot.py -e  
#renable after e stop:
rosrun baxter_tools enable_robot.py -r
#shut off sonar
rostopic pub /robot/sonar/head_sonar/set_sonars_enabled std_msgs/UInt16 0
on is 4095
#turn off collision avoidance, do for right as well
rostopic pub /robot/limb/left/suppress_collision_avoidance std_msgs/Empty -r 10
#turn on and off trajectory server for moveit
rosrun baxter_interface joint_trajectory_action_server.py

